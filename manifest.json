{
  "manifest_version": 3,
  "name": "Custom Youtube Playback Speed",
  "description": "Easy way to change youtube video playback speed",
  "version": "1.2.1",
  "short_name": "YTSpeed",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
